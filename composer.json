{
  "name": "docker/phpunit",
  "description": "Docker phpunit.",
  "type": "project",
  "require": {

  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
